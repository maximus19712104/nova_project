{
  "name": "nova-project",
  "runtime": "python-3.10",
  "entrypoint": "uvicorn run:app --host 0.0.0.0 --port 8000",
  "resources": {
    "cpu": 2,
    "memory": "4Gi",
    "gpu": false
  },
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "install": {
    "command": "pip install --no-cache-dir -r requirements.txt"
  },
  "ports": [8000],
  "description": "FastAPI backend for Nova DeepSeek Project"
}

